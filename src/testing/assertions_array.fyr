export func Test.assertArraysEqual<T>(actual []T, expected []T) {
    if (len(actual) != len(expected)) {
        this.results[current][step] = false
    }
    for (let i = 0; i < len(actual); i++) {
        if (actual[i] != expected[i]) {
            this.results[current][step] = false
        }
    }
    this.results[current][step] = true
    this.nextStep()
}

export func Test.assertArraysEqual(actual []bool, expected []bool) {
    this.assertArraysEqual<bool>(actual, expected)
}
