{
  "name": "fyr",
  "version": "0.0.2",
  "description": "Fyr: an experimental compiler for WebAssembly and IoT applications",
  "main": "lib/index",
  "typings": "lib/index",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:parser": "pegjs -o lib/parser.js compiler/parser.pegjs",
    "build:js": "tsc",
    "build:lib": "mkdir -p pkg/`bin/fyrarch` && gcc -o pkg/`bin/fyrarch`/fyr.o -O3 -g3 -c src/runtime/fyr.c",
    "build": "npm run build:parser && npm run build:js && npm run build:lib",
    "clean": "rm lib/*"
  },
  "keywords": [
    "fyr",
    "wasm",
    "webassembly",
    "compiler"
  ],
  "author": "Torben Weis",
  "license": "Apache-2.0",
  "dependencies": {
    "colors": "^1.3.0",
    "commander": "^2.9.0",
    "text-encoding": "^0.6.4"
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/node": "^8.0.0",
    "pegjs": "^0.10.0"
  }
}
