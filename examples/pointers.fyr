type Point struct {
    x int
    y int
}

func &Point.translate(x int, y int) {
    this.x += x
    this.y += y
}

func add(p1 const box &Point, p2 const box &Point) Point {
    return {x: p1.x + p2.x, y: p1.y + p2.y}
}

