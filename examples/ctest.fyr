var n int

type Point struct {
    x int
    y int
}

func const &Point.zero() bool {
    return this.x == 0 && this.y == 0
}

func doSomething(s string) int {
    return s.len()
}

func somePoint() Point {
    var p Point = {x:42, y:24}
    p.x = 13
    p.y = 31
    return p
}

func otherPoint() Point {
    var p Point
    if (!p.zero()) {
        return p
    }
    p = {x:42, y:24}
    return p
}

export func main() int {
    var len = doSomething("Hello") * 2 + 1;
    return len
}