// Platzieren Sie Ihre Einstellungen in dieser Datei, um Standard- und Benutzereinstellungen zu Ã¼berschreiben.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.js": { "when": "$(basename).ts"},
        "*.js.map": true
    },
    "npm.enableScriptExplorer": true,
    "files.associations": {
        "stdio.h": "c"
    }
}